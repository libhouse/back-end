{
  "AllowedHosts": "*",
  "GeneralSettings": {
    "MITLicense": "https://opensource.org/licenses/MIT"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "MemoryCachingConfiguration": {
    "CacheSizeLimit": 200,
    "CacheEntrySize": 1
  },
  "UserPasswordResetSenderSettings": {
    "RequestPasswordResetAddress": "http://lib-house.s3-website-us-east-1.amazonaws.com/request-password-reset"
  },
  "UserRegistrationSenderSettings": {
    "ConfirmEmailAddress": "http://lib-house.s3-website-us-east-1.amazonaws.com/login"
  }
}